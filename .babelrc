{
	"sourceMaps": true,
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 9"],
        "node": "8"
      }
    }]
  ],
	"plugins": [
		"transform-runtime",
		"transform-class-properties",
		"transform-es2015-arrow-functions"
	],
	"env": {
		"test": {
			"plugins": ["istanbul"]
		}
	}
}
