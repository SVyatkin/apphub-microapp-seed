{
  "sourceMaps": true,
  "presets": [
    ["react"],
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-runtime",
    "transform-async-to-generator",
    "transform-class-properties",
    "transform-es2015-arrow-functions",
    "syntax-dynamic-import",
    "react-hot-loader/babel"
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "test": {
      "presets": ["env"]
    }
  }
}
