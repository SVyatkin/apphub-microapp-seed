{
	"sourceMaps": true,
  "presets": [
    ["react"],
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 9"
          ]
        }
      }
    ]
  ],
	"plugins": [
    "transform-runtime",
    "transform-async-to-generator",
		"transform-class-properties",
    "transform-es2015-arrow-functions",
    "react-hot-loader/babel"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    },
    "test": {
      "presets": ["env"]
    }
  }
}
